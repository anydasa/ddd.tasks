{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "bornfree/tactician-doctrine-domain-events": "^0.4.0",
    "bornfreee/tactician-domain-events-bundle": "^0.5.5",
    "doctrine/annotations": "^1.11",
    "league/tactician-bundle": "^1.1",
    "league/tactician-doctrine": "^1.1",
    "nelmio/api-doc-bundle": "^4.1",
    "ramsey/uuid-doctrine": "^1.6",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "*",
    "symfony/orm-pack": "*",
    "symfony/serializer": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "phpstan/phpstan": "^0.12.69",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "*",
    "symplify/easy-coding-standard": "^9.0",
    "vimeo/psalm": "^4.4"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },


  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2"
    }
  }
}
